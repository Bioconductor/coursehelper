<%= form_for :attendee do |f| %>
  <p>
    <%= f.label "First Name" %><br>
    <%= f.text_field :firstname %>
  </p>
 

  <p>
    <%= f.label "Last Name" %><br>
    <%= f.text_field :lastname %>
  </p>


  <p>
    <%= f.label "Email Address" %><br>
    <%= f.text_field :email %>
  </p>


 
  <p>
    <%= f.submit "Submit" %>
  </p>
<% end %>
